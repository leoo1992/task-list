{
  "singleQuote": true,
  "semi": true,
  "useTabs": false,
  "tabWidth": 2,
  "trailingComma": "all",
  "printWidth": 80,
  "bracketSpacing": true,
  "arrowParens": "always",
  "jsxSingleQuote": false,
  "embeddedLanguageFormatting": "auto",
  "jsxBracketSameLine": false,
  "singleAttributePerLine": true,
  "quoteProps": "as-needed",
  "proseWrap": "preserve",
  "htmlWhitespaceSensitivity": "css"
}
